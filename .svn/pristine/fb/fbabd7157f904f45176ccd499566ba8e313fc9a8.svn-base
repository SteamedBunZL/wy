    <style name="item_search_init">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/textsize_subtext</item>
        <item name="android:background">@drawable/selector_noradius_blue</item>
    </style>

    <style name="info_item_top">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginBottom">-1dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:background">@drawable/selector_radius_top</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <!-- <item name="android:paddingTop">3dp</item> <item name="android:textStyle">normal</item> -->

    <style name="info_item_middle">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">-1dp</item>
        <item name="android:layout_marginBottom">-1dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:background">@drawable/selector_radius_middle</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="info_item_bottom">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:background">@drawable/selector_radius_bottom</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="info_red_bottom">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:background">@drawable/rectangle_radius_white_bottom</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">normal</item>
    </style>
    <!-- <item name="android:paddingBottom">3dp</item> -->

    <style name="info_item_single">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:background">@drawable/selector_radius_white_gary</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="radio_group_top">
        <item name="android:padding">10dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="radio_button_top">
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:textColor">@drawable/selector_color_gary_white</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
    </style>

    <style name="event_detial_bottom_button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:background">@drawable/selector_button_tran_red</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="listview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingLeft">5dip</item>
        <item name="android:paddingRight">5dip</item>
        <item name="android:dividerHeight">10dip</item>
        <item name="android:divider">@android:color/transparent</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
    </style>

    <style name="loading_small" parent="@android:style/Widget.ProgressBar.Small">
        <item name="android:indeterminateDrawable">@drawable/anim_loading_gray</item>
        <item name="android:layout_width">21dip</item>
        <item name="android:layout_height">21dip</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="event_valu_gallery">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">20dip</item>
        <item name="android:layout_marginRight">20dip</item>
        <item name="android:paddingTop">10dip</item>
        <item name="android:paddingBottom">10dip</item>
        <item name="android:background">@drawable/midd</item>
        <item name="android:spacing">32dip</item>
    </style>

    <style name="event_valu_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">20dip</item>
        <item name="android:layout_marginTop">10dip</item>
        <item name="android:textSize">@dimen/textsize_text</item>
    </style>

    <style name="event_valu_disc">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/carmine_text</item>
        <item name="android:hint">请评分</item>
        <item name="android:textSize">@dimen/textsize_subtext</item>
    </style>

    <style name="event_search_filter_back">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/item_height</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">8dp</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_button_gary_blue</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="event_search_filter_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawableRight">@drawable/selector_triangle</item>
        <item name="android:textColor">@drawable/selector_color_gary_white</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="event_search_filter_texts">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@drawable/selector_color_gary_white</item>
        <item name="android:textSize">@dimen/textsize_text</item>
        <item name="android:gravity">center</item>
    </style>

</resources>