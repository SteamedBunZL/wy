public class UserSafeSettingTModel  {
	private int seeme = 1;
	private int seejoinacticity = 0;
	private float longitude;
	private float latitude;
	
	public int getSeeme() {
		return seeme;
	}
	public void setSeeme(int seeme) {
		this.seeme = seeme;
	}
	public void setSeeme(boolean isChecked) {
		if(isChecked){
			seeme = 1;
		}else{
			seeme = 0;
		}
	}
	public int getSeejoinacticity() {
		return seejoinacticity;
	}
	public void setSeejoinacticity(int seejoinacticity) {
		this.seejoinacticity = seejoinacticity;
	}
	public void setSeejoinacticity(boolean isChecked) {
		if(isChecked){
			seejoinacticity = 1;
		}else{
			seejoinacticity = 0;
		}
	}
	public float getLongitude() {
		return longitude;
	}
	public void setLongitude(float longitude) {
		this.longitude = longitude;
	}
	public float getLatitude() {
		return latitude;
	}
	public void setLatitude(float latitude) {
		this.latitude = latitude;
	}
}
