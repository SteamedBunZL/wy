package com.whoyao.model;

import java.util.ArrayList;

public class MyFundRModel {
	private double packetamount;
	private double amount;
	private int packetcount;
	private ArrayList<MyFundRedList> UserPacketItem;

	public double getPacketamount() {
		return packetamount;
	}

	public void setPacketamount(double packetamount) {
		this.packetamount = packetamount;
	}

	public double getAmount() {
		return amount;
	}

	public void setAmount(double amount) {
		this.amount = amount;
	}

	public int getPacketcount() {
		return packetcount;
	}

	public void setPacketcount(int packetcount) {
		this.packetcount = packetcount;
	}

	public ArrayList<MyFundRedList> getUserPacketItem() {
		return UserPacketItem;
	}

	public void setUserPacketItem(ArrayList<MyFundRedList> userPacketItem) {
		UserPacketItem = userPacketItem;
	}

	

}
